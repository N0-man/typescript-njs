    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "njs-types": "^0.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.46.0",
